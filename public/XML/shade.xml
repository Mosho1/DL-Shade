<Shade>
    <Node>
        <UI>TabSet</UI>
        <TabStripPlacement>Bottom</TabStripPlacement>
        <vActiveTabIndex>x</vActiveTabIndex>
        <Sub>
            <Node>
                <UI>Tab</UI>
                <Text>Standard</Text>
                <Sub>
                    <Node>
                        <UI>Button</UI>
                        <Text>Change active item to 1</Text>
                        <Cb>
                            <Fn>SETDLVARIABLE</Fn>
                            <Stat>x, 1</Stat>
                        </Cb>
                    </Node>
                </Sub>
            </Node>
            <Node>
                <UI>Tab</UI>
                <Text>Text Tag</Text>
                <Sub>
                    <Node>
                        <UI>Button</UI>
                        <Text>Change active item to 0</Text>
                        <Cb>
                            <Fn>SETDLVARIABLE</Fn>
                            <Stat>x, 0</Stat>
                        </Cb>
                    </Node>
                </Sub>
            </Node>
            <Node>
                <UI>Tab</UI>
                <Text>Instead Of</Text>
            </Node>
            <Node>
                <UI>Tab</UI>
                <Text>Caption</Text>
            </Node>
        </Sub>
    </Node>
    <Node><UI>NumEdit</UI><vText>x</vText></Node>
    <Node><UI>TestDL</UI><vDL>x</vDL></Node>
</Shade>